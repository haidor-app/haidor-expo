{
  "cli": {
    "version": ">= 0.57.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "extends": "production",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "APPLICATION_ID": "com.zabrador.haidor.dev"
      }
    },
    "production": {
      "channel": "master",
      "autoIncrement": true,
      "env": {
        "APPLICATION_ID": "com.zabrador.haidor"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
